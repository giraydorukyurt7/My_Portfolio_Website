@model List<My_Portfolio_Website.DAL.Entities.Skill>

@{
    int x = 0;
}

<div class="col-six tab-full right">
    <h3>I've Got Some skills.</h3>

    <ul class="skill-bars">
        @foreach (var item in Model)
        {
            <li>
                <div class="progress percent@(item.Value.ToString())"><span>@item.Value</span></div>
                <strong>@item.Title</strong>
            </li>
        }
        </li>
    </ul>
</div>